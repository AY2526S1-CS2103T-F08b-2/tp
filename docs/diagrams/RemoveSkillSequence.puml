@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":RemoveSkillCommandParser" as RemoveSkillCommandParser LOGIC_COLOR
participant ":RemoveSkillCommand" as RemoveSkillCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("removeSkill p/1 sk/Java")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand(...)
activate AddressBookParser

create RemoveSkillCommandParser
AddressBookParser -> RemoveSkillCommandParser
activate RemoveSkillCommandParser

RemoveSkillCommandParser --> AddressBookParser
deactivate RemoveSkillCommandParser

AddressBookParser -> RemoveSkillCommandParser : parse(...)
activate RemoveSkillCommandParser

create RemoveSkillCommand
RemoveSkillCommandParser -> RemoveSkillCommand
activate RemoveSkillCommand

RemoveSkillCommand --> RemoveSkillCommandParser :
deactivate RemoveSkillCommand

RemoveSkillCommandParser --> AddressBookParser :
deactivate RemoveSkillCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
RemoveSkillCommandParser -[hidden]-> AddressBookParser
destroy RemoveSkillCommandParser

AddressBookParser --> LogicManager :
deactivate AddressBookParser

LogicManager -> RemoveSkillCommand : execute(...)
activate RemoveSkillCommand

RemoveSkillCommand -> Model : update person in model
activate Model

Model --> RemoveSkillCommand :

deactivate Model

create CommandResult
RemoveSkillCommand -> CommandResult
activate CommandResult

CommandResult --> RemoveSkillCommand
deactivate CommandResult

RemoveSkillCommand --> LogicManager : r
deactivate RemoveSkillCommand

[<--LogicManager
deactivate LogicManager
@enduml
